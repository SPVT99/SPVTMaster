@{ 
    Layout = "/Views/Shared/_Layout.cshtml";
}

<h2 class="title">Manage your account</h2>

<div class="viewbox">
    <h3>Change your account settings</h3>
    <br />
    <div class="row">
        <div class="col-md-3">
            @await Html.PartialAsync("_ManageNav")
        </div>
        <div class="col-md-9">
            @RenderBody()
        </div>
    </div>
</div>

@section Scripts {
    @RenderSection("Scripts", required: false)
}
        
